\end_layout

\begin_layout Itemize
refinar DCD
\end_layout

\begin_layout Itemize
refinar Arquitectura SW 
\end_layout

\begin_layout Itemize
implementación UC (75%)
\end_layout

\begin_layout Itemize
pruebas UC (75%)
\end_layout

\begin_layout Subsubsection
